#32
N necro_ethereal_aid~
C
if ( reagents({34 42 45 61 91 90 140 135 116 117 210 279 282},{2 1 1 1 2 1 2 2 1 2 1 }),
     {
         goto(mix);
     },
     {
         goto(fail);
     } );
label(fail);
