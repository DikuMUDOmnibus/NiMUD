#545
N vlazgad-to-trynt~
C
* ferryman2
label(ferryman-start);
wait(10);
do({:quietly steers the ferry past some rocks.});
wait(30);
do({say I can take you only as far as Trynt.});
wait(10);
do({say From there, you will either have to travel by caravan.});
wait(30);
do({:points to the smoke up ahead.});
wait(10);
do({We are approaching the dock.  I will land near where the Cetbrok meets the Kyr river, where you must take the road.});
wait(20);
echo({The boat is approaching port.});
wait(20);
do({:ties the ferry to the dock.});
wait(10);
do({say Enjoy your journey.  It is not far to the east that you will find Trynt, a fishing village on the Cetbrok.});
wait(15);
do({say From there, you can walk to Kess.});
wait(10);
moveall(545,600,{look});
~
T 0
End

#549
N tanzer-choice~
C
* tanzer-choice
wait(5);
if( cmp(%astr%,{veteran}),
    {
       heal( %name%, 100 ); 
       move(%name%,551);
       do({:sends off a veteran.});  
       goto(end);
    } );
if( cmp(%astr%, {easy}),
    {
        heal(%name%,100);
        do({grunt});;
        move(%name%,552);
        goto(end);
    } );
if ( cmp(%astr%, {duel}),
     {
         do({:nods.  Looks around for challengers.});
         move(%name%,564);
         goto(end);
     } );
if ( cmp(%astr%, {deathmatch}),
     {
         do({:laughs mercilessly.});
         move(%name%, 553);
         goto(end);
     } );
do({say Eh?  Easy, veteran or deathmatch?!  Or a duel?});
do({give scroll %name%});
label(end);
~
T 6
End

